<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Casino Games</title>
  <link href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.css" rel="stylesheet">
</head>
<body class="bg-gray-900 text-white">
  <!-- Header -->
  <header class="bg-gray-800 p-4">
    <h1 class="text-2xl text-center">Casino Games</h1>
  </header>

  <!-- Slot Games Section -->
  <section class="p-4">
    <h2 class="text-xl mb-4">🎰 Slot Games</h2>
    <div class="grid grid-cols-2 md:grid-cols-4 gap-4">
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/BookofRa/200/300" alt="Book of Ra" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Book of Ra</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/Starburst/200/300" alt="Starburst" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Starburst</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/GonzosQuest/200/300" alt="Gonzo's Quest" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Gonzo's Quest</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/MegaMoolah/200/300" alt="Mega Moolah" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Mega Moolah</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/WolfGold/200/300" alt="Wolf Gold" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Wolf Gold</p>
      </div>
    </div>
  </section>

  <!-- Table Games Section -->
  <section class="p-4">
    <h2 class="text-xl mb-4">🎲 Table Games</h2>
    <div class="grid grid-cols-2 md:grid-cols-4 gap-4">
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/Blackjack/200/300" alt="Blackjack" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Blackjack</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/Roulette/200/300" alt="Roulette" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Roulette</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/Baccarat/200/300" alt="Baccarat" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Baccarat</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/Craps/200/300" alt="Craps" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Craps</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/ThreeCardPoker/200/300" alt="Three Card Poker" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Three Card Poker</p>
      </div>
    </div>
  </section>

  <!-- Poker Variants Section -->
  <section class="p-4">
    <h2 class="text-xl mb-4">🃏 Poker Variants</h2>
    <div class="grid grid-cols-2 md:grid-cols-4 gap-4">
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/TexasHoldem/200/300" alt="Texas Hold'em" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Texas Hold'em</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/OmahaPoker/200/300" alt="Omaha Poker" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Omaha Poker</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/CaribbeanStudPoker/200/300" alt="Caribbean Stud Poker" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Caribbean Stud Poker</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/SevenCardStud/200/300" alt="Seven-Card Stud" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Seven-Card Stud</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/PaiGowPoker/200/300" alt="Pai Gow Poker" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Pai Gow Poker</p>
      </div>
    </div>
  </section>

  <!-- Specialty Games Section -->
  <section class="p-4">
    <h2 class="text-xl mb-4">🎮 Specialty Games</h2>
    <div class="grid grid-cols-2 md:grid-cols-4 gap-4">
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/Keno/200/300" alt="Keno" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Keno</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/SicBo/200/300" alt="Sic Bo" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Sic Bo</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/DreamCatcher/200/300" alt="Dream Catcher (Live Casino)" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Dream Catcher (Live Casino)</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/LightningDice/200/300" alt="Lightning Dice" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Lightning Dice</p>
      </div>
      <div class="bg-white p-4 rounded shadow">
        <img src="https://picsum.photos/seed/MonopolyLive/200/300" alt="Monopoly Live" class="w-full h-48 object-cover">
        <p class="text-center mt-2 text-gray-800">Monopoly Live</p>
      </div>
    </div>
  </section>
</body>
</html>